<%= form_for @item_category, url: { action: "create" }  do |f| %>
  <% if @item_category.errors.any? %>
    <% @item_category.errors.full_messages.each do |message| %>
      <li class="error_message"><%= message %></li>
    <% end %>
  <% end %>
  <%= f.label :Name %>
  <%= f.text_field :name %>
  <%= f.submit "Create ItemCategory" %>
<% end %>
<%= link_to "Back", item_categories_path %>