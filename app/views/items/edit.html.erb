<%= form_for @item, url: { action: "update" }  do |f| %>
  <% if @item.errors.any? %>
    <% @item.errors.full_messages.each do |message| %>
      <li class="error_message"><%= message %></li>
    <% end %>
  <% end %>
  <%= f.label :ItemCategoryId %>
  <%= f.text_field :item_category_id %>
  <%= f.label :Name %>
  <%= f.text_field :name%>
  <%= f.label :Rate %>
  <%= f.text_field :rate %>
  <%= f.submit "Update Item" %>
<% end %>
<%= link_to "Back", items_path%>